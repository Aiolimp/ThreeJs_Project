original":{"line":70,"column":4}},{"generated":{"line":77,"column":2},"source":"main/main.js","original":{"line":71,"column":4},"name":"renderer"},{"generated":{"line":77,"column":10},"source":"main/main.js","original":{"line":71,"column":12}},{"generated":{"line":77,"column":11},"source":"main/main.js","original":{"line":71,"column":13},"name":"render"},{"generated":{"line":77,"column":17},"source":"main/main.js","original":{"line":71,"column":19}},{"generated":{"line":77,"column":18},"source":"main/main.js","original":{"line":71,"column":20},"name":"scent"},{"generated":{"line":77,"column":23},"source":"main/main.js","original":{"line":71,"column":25}},{"generated":{"line":77,"column":25},"source":"main/main.js","original":{"line":71,"column":27},"name":"camera"},{"generated":{"line":77,"column":31},"source":"main/main.js","original":{"line":71,"column":33}},{"generated":{"line":77,"column":32},"source":"main/main.js","original":{"line":71,"column":34}},{"generated":{"line":78,"column":2},"source":"main/main.js","original":{"line":72,"column":4}},{"generated":{"line":79,"column":2},"source":"main/main.js","original":{"line":73,"column":4},"name":"requestAnimationFrame"},{"generated":{"line":79,"column":23},"source":"main/main.js","original":{"line":73,"column":25}},{"generated":{"line":79,"column":24},"source":"main/main.js","original":{"line":73,"column":26},"name":"render"},{"generated":{"line":79,"column":30},"source":"main/main.js","original":{"line":73,"column":32}},{"generated":{"line":79,"column":31},"source":"main/main.js","original":{"line":73,"column":33}},{"generated":{"line":80,"column":0},"source":"main/main.js","original":{"line":74,"column":0}},{"generated":{"line":81,"column":0},"source":"main/main.js","original":{"line":75,"column":0},"name":"render"},{"generated":{"line":81,"column":6},"source":"main/main.js","original":{"line":75,"column":6}},{"generated":{"line":81,"column":8},"source":"main/main.js","original":{"line":75,"column":8}}],"sources":{"main/main.js":"import * as THREE from 'three'\r\n// 导入轨道控制器\r\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls';\r\n\r\n// console.log(THREE)\r\n\r\n// 目标：clock跟踪时间处理动画\r\n\r\n// 1.创建场景\r\nconst scent = new THREE.Scene();\r\n\r\n// 2.创建相机：透视相机（PerspectiveCamera）\r\nconst camera = new THREE.PerspectiveCamera(75, window.innerWidth / window.innerHeight, 0.1, 1000)\r\n\r\n//   2.1设置相机位置\r\ncamera.position.set(0, 0, 10);\r\n//   2.2将相机添加到场景中\r\nscent.add(camera)\r\n\r\n// 3.添加物体\r\n//   3.1添加几何体和材质：立方缓冲几何体（BoxGeometry）、基础网格材质(MeshBasicMaterial)\r\nconst cubeGeometry = new THREE.BoxGeometry(1, 1, 1);\r\nconst cubeMaterial = new THREE.MeshBasicMaterial({ color: 0xffff00 })\r\n\r\n//   3.2根据几何体和材质创建物体：网格（Mesh）\r\nconst cube = new THREE.Mesh(cubeGeometry, cubeMaterial)\r\n\r\n//   3.3修改物体的位置:position\r\n// cube.position.set(5,0,0)\r\n// cube.position.x = 3\r\n//   3.5设置物体的缩放:scale\r\n// cube.scale.set(3,2,1)\r\n// cube.scale.x = 2\r\n//   3.6设置物体的旋转:rotation (Math.PI/4 = 45°)\r\ncube.rotation.set(Math.PI / 4, 0, 0)\r\n\r\n//   3.7将几何体添加到场景中\r\nscent.add(cube)\r\n\r\n// 4.初始化渲染器\r\nconst renderer = new THREE.WebGLRenderer();\r\n//   4.1设置渲染器尺寸大小\r\nrenderer.setSize(window.innerWidth, window.innerHeight);\r\n//renderer.domElement是一个canvas画布\r\n//  4.2将webgl渲染的canvas内容添加到body\r\ndocument.body.appendChild(renderer.domElement)\r\n\r\n// //  4.3使用渲染器，将场景通过相机渲染进来\r\n// // renderer.render(scent, camera)\r\n\r\n// 5.使用轨道控制器查看物体\r\nconst controls = new OrbitControls(camera, renderer.domElement)\r\n\r\n// 6.添加坐标轴辅助器(AxesHelper)：参数代表轴的线段长度. 默认为 1.\r\nconst axesHelper = new THREE.AxesHelper(5)\r\n//   将坐标轴辅助器体添加到场景中\r\nscent.add(axesHelper)\r\n\r\n// 设置时钟\r\nconst clock = new THREE.Clock()\r\nfunction render() {\r\n    // let deltaTime = clock.getDelta() // 获取两次时间的间隔时间\r\n    // 获取时钟运行总时长\r\n    let time = clock.getElapsedTime()\r\n    let t = time % 5 // 这里的time是秒不需要除以1000，time取模5的值，就是在X轴上需要移动的时间\r\n    cube.position.x = t * 1 //移动的时间 * 速度 = t * 1 = 在x轴移动的距离\r\n    // 如果移动距离大于5、则返回起始点\r\n    if (cube.position.x > 5) {\r\n        cube.position.x = 0\r\n    }\r\n    renderer.render(scent, camera)\r\n    // 渲染下一帧的时候就会调用render函数\r\n    requestAnimationFrame(render)\r\n}\r\nrender()"},"lineCount":null}},"error":null,"hash":"add918ec62c83e796a318c6ebb0c1571","cacheData":{"env":{}}}